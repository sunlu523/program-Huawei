  });
  if ($('#pro-detail-contents *[data-src]').length > 0) $('.pro-detail-see').css('height', '500000px');
  var hiddenImgs = [];
  var imgsHeight = 0;
  $('#pro-detail-contents *[data-src]').each(function() {
    hiddenImgs.push($(this).attr('data-src'));
  });
  var showDetailImgs = function() {
    var top = $(window).height() + $(document).scrollTop();
    $('#pro-detail-contents *[data-src]').each(function() {
      var $img = $(this);
      var url = $img.attr('data-src');
      if (url && $img.offset().top < top) {
        $img.attr('src', url).removeAttr('data-src');
        if (hiddenImgs.indexOf(url) > -1) {
          var imgLoader = new Image();
          imgLoader.onload = function() {
            hiddenImgs = hiddenImgs.filter(function(v) {
              return v !== url;
            });
            imgsHeight += $(imgLoader).height();
            if (imgsHeight > 300000 || hiddenImgs.length < 1) {
              $('.pro-detail-see').css('height', 'auto');
            }
          }
          imgLoader.onerror = function() {
            hiddenImgs = hiddenImgs.filter(function(v) {
              return v !== url;
            });
            if (hiddenImgs.length < 1) {
              $('.pro-detail-see').css('height', 'auto');
            }
          }
          imgLoader.src = url;
        }
      }
    });
  };
  showDetailImgs();
  $(document).on('scroll', _.debounce(showDetailImgs, 200));
});
