        url: openApiDomain + '/mcp/queryPrd',
        data: data,
        success: function(r) {
          if (r.labelList) {
            var cids = _.map(ec.searchPage.labels, 'category');
            r.labelList.forEach(function(c) {
              if (cids.indexOf(c.category) > -1) {
                ec.searchPage.labels.forEach(function(v) {
                  if (v.category === c.category && (!v.labelContentList || v.labelContentList.length < 1)) {
                    v.labelContentList = c.labelContentList;
                  }
                });
              } else {
                ec.searchPage.labels.push(c);
                cids.push(c.category);
              }
            });
          }
          if (searchObj.isChildCategory) {
            searchObj.query.keyword = _.last(_cate).id
            searchObj.query.searchLabel = searchObj.query.searchLabel || {}
            searchObj.query.searchLabel.category = data.keyword
            searchObj.isChildCategory = false
          }
